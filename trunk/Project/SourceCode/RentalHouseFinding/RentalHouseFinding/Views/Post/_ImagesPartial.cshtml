@model IEnumerable<RentalHouseFinding.Models.PostImages>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jcarousel.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.pikachoose.full.js")" type="text/javascript"></script>
</head>

<script language="javascript">
	$(document).ready(
		function () {
			$("#images").PikaChoose();

		});
</script>
<ul id="Images">
    @foreach (var item in Model)
    {
        <img src="@Html.DisplayFor(modelItem => item.Path)" alt="@Html.DisplayFor(modelItem => item.Path)" />
    }
</ul>

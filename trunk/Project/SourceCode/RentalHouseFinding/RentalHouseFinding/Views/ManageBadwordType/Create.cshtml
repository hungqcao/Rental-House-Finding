@model RentalHouseFinding.Models.BadWordTypes

@{
    ViewBag.Title = "Tạo phân loại từ xấu";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
         <div class="districtDiv">

        <div class="editor-label">
            @Html.LabelFor(model => model.Name, "Loại từ xấu mới")
        </div>
        <div class="editor-field text-box single-line" id="Name">
            @Html.EditorFor(model => model.Name)
            @Html.ValidationMessageFor(model => model.Name)
        </div>
</div>
  <div class="rolesManageSubmit">
        <p>
            <input type="submit" class="createLink" value="Tạo mới" />
        </p>
        </div>
    </fieldset>
}

 
    <div class="backLink"><a href="../ManageBadWordType/Index"><img src="../../Content/images/Back.png" /></a>
    </div>

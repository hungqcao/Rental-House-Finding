@model IEnumerable<RentalHouseFinding.Models.Favorites>
@{
    this.Layout = null;
    int i = 0;
}

@foreach (var item in Model)
{
    i++;
    <div class="result">
        <span><b>@i.</b> <a href="@Url.Content("~/post/details/" + @item.PostId)">@item.Post.Title</a></span>
        <input id="@item.PostId" type="checkbox" class="styledCheckbox favItem" />
    </div>
}